{
    "name": "tadasei/laravel-crud-stubs",
    "description": "A package that provides stubs for crud backend using inertia, blade and api stacks",
	"keywords": ["laravel", "crud", "stubs", "backend"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tadasei",
            "email": "chemselhak.mahouche@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2.0",
        "illuminate/console": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Tadasei\\LaravelCrudStubs\\": "src/"
        }
    },
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Tadasei\\LaravelCrudStubs\\ServiceProvider"
			]
		}
	}
}
